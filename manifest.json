{
    "name": "Max Extension 1",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
          "storage"
        ],
    "content_scripts": [
        {
            "all_frames": true,
            "matches": ["*://vimeo.com/*", "*://player.vimeo.com/*"],
            "js": ["content.js"],
            "css": ["styles.css"],
            "run_at": "document_idle"
        }
    ],
    "page_action": {
          "default_icon": "images/defaultIcon.png"
    }
}
